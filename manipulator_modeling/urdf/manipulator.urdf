<?xml version="1.0" ?>
<robot name="manipulator">

  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="red">
    <color rgba="1.0 0.0 0.0 1.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 1.0 0.0 1.0"/>
  </material>

  <link name="base"/>

  <joint name="fixed" type="fixed">
    <parent link="base"/>
    <child link="link1"/>
  </joint>

  <link name="link1">
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 0.2" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0" />
            <geometry>
                <box size="0.1 0.1 0.2" />
            </geometry>
            <material name="black" />
        </visual>
        <inertial>
            <origin xyz="0 0 0.25" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>

    <joint name="joint1" type="revolute">
        <parent link="link1" />
        <child link="link2" />
        <origin xyz="0 0 0.5" rpy="0 0 0"/>
        <axis xyz="0 01 0"/>
        <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571" />
    </joint>


    <link name="link2">
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 0.4" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0.25" rpy="0 0 0" />
            <geometry>
                <box size="0.1 0.1 0.4" />
            </geometry>
            <material name="red" />
        </visual>
        <inertial>
            <origin xyz="0 0 0.25" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>

    <joint name="joint2" type="revolute">
        <parent link="link2" />
        <child link="link3" />
        <origin xyz="0 0 0.5" rpy="0 0 0"/>
        <axis xyz="0 01 0" />
        <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571" />
    </joint>

    <link name="link3">
        <collision>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 0.6" />
            </geometry>
        </collision>
        <visual>
            <origin xyz="0 0 0.5" rpy="0 0 0" />
            <geometry>
                <box size="0.1 0.1 0.6" />
            </geometry>
            <material name="green" />
        </visual>
        <inertial>
            <origin xyz="0 0 0.5" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
        </inertial>
    </link>
</robot>
